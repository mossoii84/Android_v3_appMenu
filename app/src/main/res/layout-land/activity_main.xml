<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">


    <TextView
        android:id="@+id/textViewLand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Activity-Land"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.624"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linerForVerticalMenuLand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#6A2196F3"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.928"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497">


        <ImageView
            android:id="@+id/imageView1Land"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:background="#3F51B5"
            android:padding="5dp"
            app:srcCompat="@drawable/ic_baseline_add_24"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/imageView2Land"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:background="#3F51B5"
            android:padding="5dp"
            app:srcCompat="@drawable/ic_baseline_add_ic_call_24"
            tools:ignore="VectorDrawableCompat" />

        <ImageView
            android:id="@+id/imageView3Land"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:background="#3F51B5"
            android:padding="5dp"
            app:srcCompat="@drawable/ic_baseline_add_24"
            tools:ignore="VectorDrawableCompat" />
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>